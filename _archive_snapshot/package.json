{
  "name": "supplysense",
  "version": "1.0.0",
  "description": "Multi-Agent Supply Chain Resilience System",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "deploy-system": "bash scripts/deploy-complete-system.sh",
    "test-deployment": "node scripts/test-deployment.js",
    "seed-data": "node scripts/seed-data.js",
    "validate-system": "node scripts/validate-system.js",
    "quick-test": "bash scripts/quick-test.sh",
    "setup-agentcore": "bash setup-agentcore-sdk.sh",
    "cdk": "npx cdk",
    "bootstrap": "npx cdk bootstrap",
    "deploy": "npx cdk deploy --all --require-approval never",
    "deploy-interactive": "npx cdk deploy --all",
    "destroy": "npx cdk destroy --all",
    "dev": "cd ui && npm run dev"
  },
  "keywords": [
    "supply-chain",
    "ai",
    "agents",
    "aws",
    "bedrock"
  ],
  "author": "SupplySense Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent": "^3.400.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.400.0",
    "@aws-sdk/client-cloudformation": "^3.925.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.400.0",
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/client-sagemaker": "^3.400.0",
    "@aws-sdk/client-ssm": "^3.925.0",
    "@aws-sdk/lib-dynamodb": "^3.400.0",
    "aws-cdk-lib": "^2.100.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.13.2",
    "constructs": "^10.3.0"
  }
}
